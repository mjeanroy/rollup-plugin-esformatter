{
  "presets": [
    [
      "@babel/preset-env", {
        "modules": "cjs"
      }
    ]
  ],

  "plugins": [
    "add-module-exports"
  ],

  "env": {
    "mjs": {
      "presets": [
        [
          "@babel/preset-env", {
            "modules": "false"
          }
        ]
      ]
    }
  }
}
